    function approve(address spender, uint256 value) external returns (bool);

    function transfer(address to, uint256 value) external returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 value
    ) external returns (bool);
}

library EnumerableMap {
    // To implement this library for multiple types with as little code
    // repetition as possible, we write it in terms of a generic Map type with
    // bytes32 keys and values.
    // The Map implementation uses private functions, and user-facing
    // implementations (such as Uint256ToAddressMap) are just wrappers around
    // the underlying Map.
    // This means that we can only create new EnumerableMaps for types that fit
    // in bytes32.

    struct MapEntry {
        bytes32 _key;
        bytes32 _value;
    }

    struct Map {
        // Storage of map keys and values
        MapEntry[] _entries;
        // Position of the entry defined by a key in the `entries` array, plus 1
        // because index 0 means a key is not in the map.
        mapping(bytes32 => uint256) _indexes;
    }

    /**
     * @dev Adds a key-value pair to a map, or updates the value for an existing
     * key. O(1).
     *
     * Returns true if the key was added to the map, that is if it was not
     * already present.
     */
    function _set(
        Map storage map,
        bytes32 key,
        bytes32 value
    ) private returns (bool) {
        // We read and store the key's index to prevent multiple reads from the same storage slot
        uint256 keyIndex = map._indexes[key];

        if (keyIndex == 0) {
            // Equivalent to !contains(map, key)
            map._entries.push(MapEntry({_key: key, _value: value}));
            // The entry is stored at length-1, but we add 1 to all indexes
            // and use 0 as a sentinel value
            map._indexes[key] = map._entries.length;
            return true;
        } else {
            map._entries[keyIndex - 1]._value = value;
            return false;
        }
    }

    /**
     * @dev Removes a key-value pair from a map. O(1).
     *
     * Returns true if the key was removed from the map, that is if it was present.
     */
    function _remove(Map storage map, bytes32 key) private returns (bool) {
        // We read and store the key's index to prevent multiple reads from the same storage slot
        uint256 keyIndex = map._indexes[key];

        if (keyIndex != 0) {
            // Equivalent to contains(map, key)
            // To delete a key-value pair from the _entries array in O(1), we swap the entry to delete with the last one
            // in the array, and then remove the last entry (sometimes called as 'swap and pop').
            // This modifies the order of the array, as noted in {at}.

            uint256 toDeleteIndex = keyIndex - 1;
            uint256 lastIndex = map._entries.length - 1;

            // When the entry to delete is the last one, the swap operation is unnecessary. However, since this occurs
            // so rarely, we still do the swap anyway to avoid the gas cost of adding an 'if' statement.

            MapEntry storage lastEntry = map._entries[lastIndex];

            // Move the last entry to the index where the entry to delete is
            map._entries[toDeleteIndex] = lastEntry;
            // Update the index for the moved entry
            map._indexes[lastEntry._key] = toDeleteIndex + 1; // All indexes are 1-based

            // Delete the slot where the moved entry was stored
            map._entries.pop();

            // Delete the index for the deleted slot
            delete map._indexes[key];

            return true;
        } else {
            return false;
        }
    }

    /**
     * @dev Returns true if the key is in the map. O(1).
     */
    function _contains(Map storage map, bytes32 key)
        private
        view
        returns (bool)
    {
        return map._indexes[key] != 0;
    }

    /**
     * @dev Returns the number of key-value pairs in the map. O(1).
     */
    function _length(Map storage map) private view returns (uint256) {
        return map._entries.length;
    }

    /**
     * @dev Returns the key-value pair stored at position `index` in the map. O(1).
     *
     * Note that there are no guarantees on the ordering of entries inside the
     * array, and it may change when more entries are added or removed.
     *
     * Requirements:
     *
     * - `index` must be strictly less than {length}.
     */
    function _at(Map storage map, uint256 index)
        private
        view
        returns (bytes32, bytes32)
    {
        require(
            map._entries.length > index,
            "EnumerableMap: index out of bounds"
        );

        MapEntry storage entry = map._entries[index];
        return (entry._key, entry._value);
    }

    /**
     * @dev Returns the value associated with `key`.  O(1).
     *
     * Requirements:
     *
     * - `key` must be in the map.
     */
    function _get(Map storage map, bytes32 key) private view returns (bytes32) {
        return _get(map, key, "EnumerableMap: nonexistent key");
    }

    /**
     * @dev Same as {_get}, with a custom error message when `key` is not in the map.
     */
    function _get(
        Map storage map,
        bytes32 key,
        string memory errorMessage
    ) private view returns (bytes32) {
        uint256 keyIndex = map._indexes[key];
        require(keyIndex != 0, errorMessage); // Equivalent to contains(map, key)
        return map._entries[keyIndex - 1]._value; // All indexes are 1-based
    }

    // UintToAddressMap

    struct UintToAddressMap {
        Map _inner;
    }

    /**
     * @dev Adds a key-value pair to a map, or updates the value for an existing
     * key. O(1).
     *
     * Returns true if the key was added to the map, that is if it was not
     * already present.
     */
    function set(
        UintToAddressMap storage map,
        uint256 key,
        address value
    ) internal returns (bool) {
        return _set(map._inner, bytes32(key), bytes32(uint256(value)));
    }

    /**
     * @dev Removes a value from a set. O(1).
     *
     * Returns true if the key was removed from the map, that is if it was present.
     */
    function remove(UintToAddressMap storage map, uint256 key)
        internal
        returns (bool)
    {
        return _remove(map._inner, bytes32(key));
    }

    /**
     * @dev Returns true if the key is in the map. O(1).
     */
    function contains(UintToAddressMap storage map, uint256 key)
        internal
        view
        returns (bool)
    {
        return _contains(map._inner, bytes32(key));
    }

    /**
     * @dev Returns the number of elements in the map. O(1).
     */
    function length(UintToAddressMap storage map)
        internal
        view
        returns (uint256)
    {
        return _length(map._inner);
    }

    /**
     * @dev Returns the element stored at position `index` in the set. O(1).
     * Note that there are no guarantees on the ordering of values inside the
     * array, and it may change when more values are added or removed.
     *
     * Requirements:
     *
     * - `index` must be strictly less than {length}.
     */
    function at(UintToAddressMap storage map, uint256 index)
        internal
        view
        returns (uint256, address)
    {
        (bytes32 key, bytes32 value) = _at(map._inner, index);
        return (uint256(key), address(uint256(value)));
    }

    /**
     * @dev Returns the value associated with `key`.  O(1).
     *
     * Requirements:
     *
     * - `key` must be in the map.
     */
    function get(UintToAddressMap storage map, uint256 key)
        internal
        view
        returns (address)
    {
        return address(uint256(_get(map._inner, bytes32(key))));
    }

    /**
     * @dev Same as {get}, with a custom error message when `key` is not in the map.
     */
    function get(
        UintToAddressMap storage map,
        uint256 key,
        string memory errorMessage
    ) internal view returns (address) {
        return address(uint256(_get(map._inner, bytes32(key), errorMessage)));
    }
}

/**
 * @dev String operations.
 */
library Strings {
    /**
     * @dev Converts a `uint256` to its ASCII `string` representation.
     */
    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        uint256 index = digits - 1;
        temp = value;
        while (temp != 0) {
            buffer[index--] = bytes1(uint8(48 + (temp % 10)));
            temp /= 10;
        }
        return string(buffer);
    }
}

struct KuBird {
    uint256 tokenId;
    uint256 father_id;
    uint256 mother_id;
    uint256 element;
    PartData body;
    PartData horn;
    PartData beak;
    PartData tail;
    PartData wing;
    PartData decor;
    PartData feet;
    PartData eyes;
    uint256 rarity;
    uint256 externalId;
}

struct PartData {
    uint256 partId;
    uint256 mainGen;
    uint256 subGen1;
    uint256 subGen2;
}

interface IKuBirdCore {
    function safeMintNFT(
        address _address,
        KuBird memory _bird,
        uint256 _externalId
    ) external returns (uint256);

    function safeMintNFTEx(
        address _address,
        KuBird memory _bird,
        uint256 _tokenId,
        uint256 _externalId
    ) external returns (uint256);

    function getNextTokenId() external view returns (uint256);

    function getKubirdByExternal(uint256 _externalId)
        external
        view
        returns (KuBird memory);
}

interface ManagerInterface {
    function safeNFTs(address _address) external view returns (bool);
}

contract KuBirdFarm is Ownable {
    using SafeMath for uint256;
    using Strings for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    IERC721 public kubirdNFT =
        IERC721(0x83d3fCcE90781521049c8901Adf8cC3e554B5900);
    IKuBirdCore public kubirdCore =
        IKuBirdCore(0x83d3fCcE90781521049c8901Adf8cC3e554B5900);
    IERC20 public kuToken = IERC20(0x2e2A2aEabec88A918d9eF10D2Bb5341eda854a32);
    ManagerInterface manager =
        ManagerInterface(0xB8B5294714f843426126a2B390960d5Bf55aEF21);

    uint256 public feeHatch = 3;
    uint256 public feeStone = 40;
    uint256 public feeExchangeKC = 5;
    uint256 public feeExchangeKF = 5;
    address payable public feeWallet;

    event BuyStone(uint256 quantity, address buyer);
    event ExchangeKUB(uint256 quantity, address buyer, string transRef);
    event ExchangeKF(uint256 quantity, address buyer, string transRef);
    event ExchangeKC(uint256 quantity, address buyer, string transRef);
    event Hatch(uint256 indexed externalId, uint256 tokenId, address hatcher);

    modifier onlySafeNFT() {
        require(manager.safeNFTs(msg.sender), "require SAFE Address.");
        _;
    }

    constructor(address payable _feeWallet) public {
        feeWallet = _feeWallet;
    }

    function setFeeWallet(address payable _wallet) external onlyOwner {
        feeWallet = _wallet;
    }

    function setFeeHatch(uint256 _fee) external onlyOwner {
        feeHatch = _fee;
    }

    function setFeeExchangeKC(uint256 _fee) external onlyOwner {
        feeExchangeKC = _fee;
    }
    
    function setFeeExchangeKF(uint256 _fee) external onlyOwner {
        feeExchangeKF = _fee;
    }

    function setFeeStone(uint256 _fee) external onlyOwner {
        feeStone = _fee;
    }

    function setNFT(address _addr) external onlyOwner {
        kubirdNFT = IERC721(_addr);
        kubirdCore = IKuBirdCore(_addr);
    }

    /**
     * @dev Withdraw bnb from this contract (Callable by owner only)
     */
    function withdraw(uint256 amount) external onlyOwner {
        msg.sender.transfer(amount);
    }

    /**
     * @dev Withdraw erc20 from this contract (Callable by owner only)
     */
    function withdrawERC20(
        uint256 amount,
        address _token,
        address userAddress
    ) external onlyOwner {
        IERC20(_token).transfer(userAddress, amount);
    }
    
    function buyStone(uint256[] memory _quantity, address[] memory _buyer) public onlySafeNFT{
        require(_quantity.length == _buyer.length, "not match array");
        require(_quantity.length <= 20, "too much");
        for (uint8 i = 0; i < _quantity.length; i++){
            _buyStone(_quantity[i], _buyer[i]);
        }
    }

    function _buyStone(uint256 _quantity, address _buyer) internal {
        uint256 price = _quantity.mul(feeStone).mul(10**18);
        require(
            kuToken.allowance(_buyer, address(this)) >= price,
            "Token allowance too low"
        );
        kuToken.transferFrom(_buyer, feeWallet, price);
        emit BuyStone(_quantity, _buyer);
    }

    function hatch(
        KuBird memory _bird,
        uint256 _externalId,
        address _hatcher
    ) public onlySafeNFT {
        uint256 price = feeHatch.mul(10**18);
        require(
            kuToken.allowance(_hatcher, address(this)) >= price,
            "Token allowance too low"
        );
        kuToken.transferFrom(_hatcher, feeWallet, price);
        uint256 nextTokenId = kubirdCore.getNextTokenId();
        _bird.tokenId = nextTokenId;
        _bird.externalId = _externalId;
        kubirdCore.safeMintNFTEx(_hatcher, _bird, nextTokenId, _externalId);
        emit Hatch(nextTokenId, _externalId, _hatcher);
    }

    function exchange(uint256[] memory _quantity, address[] memory _buyer, string[] memory transRefs) public onlySafeNFT {
        require(_quantity.length == _buyer.length, "not match array");
        require(_quantity.length == transRefs.length, "not match array");
        require(_quantity.length <= 20, "too much");
        for (uint8 i = 0; i < _quantity.length; i++){
            _exchange(_quantity[i], _buyer[i], transRefs[i]);
        }
    }


    function _exchange(uint256 _quantity, address _buyer, string memory _ref) internal {
        uint256 price = _quantity;
        require(
            kuToken.allowance(_buyer, address(this)) >= price,
            "Token allowance too low"
        );
        kuToken.transferFrom(_buyer, feeWallet, price);
        emit ExchangeKUB(_quantity, _buyer, _ref);
    }

    function exchangeKF(uint256[] memory _quantity, address[] memory _buyer, string[] memory transRefs) public onlySafeNFT {
        require(_quantity.length == _buyer.length, "not match array");
        require(_quantity.length <= 20, "too much");
        for (uint8 i = 0; i < _quantity.length; i++){
            _exchangeKf(_quantity[i], _buyer[i], transRefs[i]);
        }
    }


    function _exchangeKf(uint256 _quantity, address _buyer, string memory _ref) internal {
        uint256 price = _quantity;
        uint256 fee = price.mul(feeExchangeKF).div(100);
        kuToken.transfer(_buyer, price.sub(fee));
        kuToken.transfer(feeWallet, fee);
        emit ExchangeKF(price.sub(fee), _buyer, _ref);
    }

    function exchangeKC(uint256[] memory _quantity, address[] memory _buyer, string[] memory transRefs) public onlySafeNFT {
        require(_quantity.length == _buyer.length, "not match array");
        require(_quantity.length <= 20, "too much");
        for (uint8 i = 0; i < _quantity.length; i++){
            _exchangeKc(_quantity[i], _buyer[i], transRefs[i]);
        }
    }

    function _exchangeKc(uint256 _quantity, address _buyer, string memory _ref) internal {
        uint256 price = _quantity;
        uint256 fee = price.mul(feeExchangeKC).div(100);
        kuToken.transfer(_buyer, price.sub(fee));
        kuToken.transfer(feeWallet, fee);
        emit ExchangeKC(price.sub(fee), _buyer, _ref);
    }

    function setKuToken(address _addr) public onlyOwner {
        kuToken = IERC20(_addr);
    }
}
